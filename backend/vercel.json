{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/register",
      "methods": ["POST", "OPTIONS"],   
      "dest": "/server.js"
    },
    {
      "src": "/api/test",
      "methods": ["GET"],
      "dest": "/server.js"
    }
  ]
}
